<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.msb.servicepassengeruser.dao.ServicePassengerUserInfoCustomDao">
    <select id="selectByPhoneNumber" resultType="com.msb.servicepassengeruser.entity.ServicePassengerUserInfo" parameterType="java.lang.String" >
        select
        *
        from service_passenger_user_info
        where passenger_phone = #{passengerPhone}
    </select>
</mapper>